		   WHEN @Rodzaj_poradni = '2' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w02k5
		   WHEN @Rodzaj_poradni = '3' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w03k5
		   WHEN @Rodzaj_poradni = '4' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w04k5
		   WHEN @Rodzaj_poradni = '5' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w05k5 
             WHEN @Rodzaj_poradni = '6' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w06k5
		   WHEN @Rodzaj_poradni = '7' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w07k5 
		   WHEN @Rodzaj_poradni = '8' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w08k5
		   WHEN @Rodzaj_poradni = '9' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w09k5 
		   WHEN @Rodzaj_poradni = '10' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w10k5
		   WHEN @Rodzaj_poradni = '11' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w11k5 
		   WHEN @Rodzaj_poradni = '12' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w12k5
		   WHEN @Rodzaj_poradni = '13' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w13k5
		   WHEN @Rodzaj_poradni = '14' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w14k5
		   WHEN @Rodzaj_poradni = '15' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w15k5
		   --po podstawieniu view za rok 2017 zamienić poniżej na pole 'd3w16k5' (wiersz 16 - medycyna sportowa osób 65+)   
		   WHEN @Rodzaj_poradni = '16' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w16k5
		   WHEN @Rodzaj_poradni = '17' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  0
		   WHEN @Rodzaj_poradni = '18' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w18k5 
		   WHEN @Rodzaj_poradni = '19' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w19k5
		   WHEN @Rodzaj_poradni = '20' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w20k5
		   WHEN @Rodzaj_poradni = '21' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w21k5 
		   WHEN @Rodzaj_poradni = '22' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w22k5 
		   WHEN @Rodzaj_poradni = '23' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w23k5
		   WHEN @Rodzaj_poradni = '24' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w24k5
		   WHEN @Rodzaj_poradni = '25' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w25k5
		   WHEN @Rodzaj_poradni = '26' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w26k5 
		   WHEN @Rodzaj_poradni = '27' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w27k5 END AS kol4_praklek_gr,

		   -- kolumna 5 raportu
            CASE 
		   WHEN @Rodzaj_poradni = '1' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w01k6
		   WHEN @Rodzaj_poradni = '2' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w02k6
		   WHEN @Rodzaj_poradni = '3' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w03k6
		   WHEN @Rodzaj_poradni = '4' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w04k6
		   WHEN @Rodzaj_poradni = '5' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w05k6 
             WHEN @Rodzaj_poradni = '6' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w06k6
		   WHEN @Rodzaj_poradni = '7' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w07k6 
		   WHEN @Rodzaj_poradni = '8' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w08k6
		   WHEN @Rodzaj_poradni = '9' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w09k6 
		   WHEN @Rodzaj_poradni = '10' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w10k6
		   WHEN @Rodzaj_poradni = '11' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w11k6 
		   WHEN @Rodzaj_poradni = '12' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w12k6
		   WHEN @Rodzaj_poradni = '13' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w13k6
		   WHEN @Rodzaj_poradni = '14' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w14k6
		   WHEN @Rodzaj_poradni = '15' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w15k6 
		   WHEN @Rodzaj_poradni = '16' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w16k6
		   WHEN @Rodzaj_poradni = '17' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w17k6
		   WHEN @Rodzaj_poradni = '18' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w18k6 
		   WHEN @Rodzaj_poradni = '19' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w19k6
		   WHEN @Rodzaj_poradni = '20' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w20k6
		   WHEN @Rodzaj_poradni = '21' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w21k6		    
		   WHEN @Rodzaj_poradni = '22' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w22k6		    
		   WHEN @Rodzaj_poradni = '23' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w23k6
		   WHEN @Rodzaj_poradni = '24' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w24k6
		   WHEN @Rodzaj_poradni = '25' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w25k6
		   WHEN @Rodzaj_poradni = '26' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w26k6 
		   WHEN @Rodzaj_poradni = '27' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w27k6 END AS kol5_praklek_gr,
		   
		   -- kolumna 6 raportu
            CASE 
		   WHEN @Rodzaj_poradni = '1' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w01k7
		   WHEN @Rodzaj_poradni = '2' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w02k7
		   WHEN @Rodzaj_poradni = '3' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w03k7
		   WHEN @Rodzaj_poradni = '4' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w04k7
		   WHEN @Rodzaj_poradni = '5' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w05k7 
             WHEN @Rodzaj_poradni = '6' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w06k7
		   WHEN @Rodzaj_poradni = '7' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w07k7 
		   WHEN @Rodzaj_poradni = '8' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w08k7
		   WHEN @Rodzaj_poradni = '9' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w09k7 
		   WHEN @Rodzaj_poradni = '10' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w10k7
		   WHEN @Rodzaj_poradni = '11' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w11k7 
		   WHEN @Rodzaj_poradni = '12' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w12k7
		   WHEN @Rodzaj_poradni = '13' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w13k7
		   WHEN @Rodzaj_poradni = '14' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w14k7
		   WHEN @Rodzaj_poradni = '15' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w15k7 
		   WHEN @Rodzaj_poradni = '16' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w16k7
		   WHEN @Rodzaj_poradni = '17' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w17k7
		   WHEN @Rodzaj_poradni = '18' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w18k7 
		   WHEN @Rodzaj_poradni = '19' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w19k7
		   WHEN @Rodzaj_poradni = '20' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w20k7
		   WHEN @Rodzaj_poradni = '21' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w21k7		    
		   WHEN @Rodzaj_poradni = '22' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w22k7		    
		   WHEN @Rodzaj_poradni = '23' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w23k7
		   WHEN @Rodzaj_poradni = '24' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w24k7
		   WHEN @Rodzaj_poradni = '25' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w25k7
		   WHEN @Rodzaj_poradni = '26' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w26k7 
		   WHEN @Rodzaj_poradni = '27' AND KOD_RPZ = 97 AND KOD_ZL IS NOT NULL THEN  d3w27k7 END AS kol6_praklek_gr

     FROM vzd3_2021_dane
     WHERE 
	   ra = 1
     AND woj_nazwa IS NOT NULL
     AND rodz in (SELECT Param FROM dbo.fn_StringToTable(@Char_miejsc, ',') AS fn_StringToTable_1)
		 
   
GO



USE [SZ_ZD3]
GO

/****** Object:  UserDefinedFunction [dbo].[fn_StringToTable]    Script Date: 2022-02-11 12:11:45 ******/
SET ANSI_NULLS ON
GO

SET QUOTED_IDENTIFIER ON
GO

create FUNCTION [dbo].[fn_StringToTable]
   (@RepParam nvarchar(4000), @Delim char(1)= ',')
RETURNS @Values TABLE (Param nvarchar(4000))AS
  BEGIN
  DECLARE @chrind INT
  DECLARE @Piece nvarchar(100)
  SELECT @chrind = 1 
  WHILE @chrind > 0
    BEGIN
      SELECT @chrind = CHARINDEX(@Delim,@RepParam)
      IF @chrind  > 0
        SELECT @Piece = LEFT(@RepParam,@chrind - 1)
      ELSE
        SELECT @Piece = @RepParam
      INSERT  @Values(Param) VALUES(CAST(@Piece AS VARCHAR))
      SELECT @RepParam = RIGHT(@RepParam,LEN(@RepParam) - @chrind)
      IF LEN(@RepParam) = 0 BREAK
    END
  RETURN
  END
GO












