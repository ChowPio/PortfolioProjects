--BASE VIEWS


CREATE VIEW [dbo].[v_nuts_2021]
AS
SELECT [makreg] as MAKREG
      ,[woj]    as WOJ
      ,[reg]    as REG
      ,[podr]   as PODR
      ,[pow]    as POW
      ,[makreg_nazwa]
      ,[woj_nazwa]
      ,[reg_nazwa]
      ,[podr_nazwa]
      ,[pow_nazwa]
FROM [dbo].[v_teryt_nuts_2021]
group by [makreg]
        ,[woj]
        ,[reg]
        ,[podr]
        ,[pow]
        ,[makreg_nazwa]
        ,[woj_nazwa]
        ,[reg_nazwa]
        ,[podr_nazwa]
        ,[pow_nazwa]
GO







USE [SZ_ZD3]
GO


SET ANSI_NULLS ON
GO

SET QUOTED_IDENTIFIER ON
GO



CREATE VIEW [dbo].[vzd3_2021_dane]
AS
SELECT dane.roks
      ,dane.idspr
      ,dane.regon
      ,podzial_teryt.makreg as nts1
      ,podzial_teryt.podr as nts3
      ,dane.nr_kr
      ,dane.o_or
      ,dane.kod_or
      ,CAST(dane.kod_podm AS TINYINT) AS kod_podm
      ,dane.kod_fop
      ,dane.kod_jo
      ,CAST(dane.rjs_p_przy AS INT) AS rjs_p_przy
      ,CAST(dane.rjs_p_inna AS INT) AS rjs_p_inna
      ,CAST(dane.kod_rpz AS TINYINT) AS kod_rpz
      ,CAST(dane.kod_zl AS TINYINT) AS kod_zl
      ,CAST(isnull(dane.d1p1w1, 0) AS INT) AS d1p1w1
      ,CAST(isnull(dane.d1p1w2, 0) AS INT) AS d1p1w2
      ,CAST(isnull(dane.d1p1w3, 0) AS INT) AS d1p1w3
      ,CAST(isnull(dane.d1p1w4, 0) AS INT) AS d1p1w4
      ,CAST(isnull(dane.d1p1w5, 0) AS INT) AS d1p1w5
      ,CAST(isnull(dane.d1p1w6, 0) AS INT) AS d1p1w6
      ,CAST(isnull(dane.d1p1w7, 0) AS INT) AS d1p1w7
      ,CAST(isnull(dane.d1p1w8, 0) AS INT) AS d1p1w8
      ,CAST(isnull(dane.d1p1w9, 0) AS INT) AS d1p1w9
      ,CAST(isnull(dane.d1p1w10,0) AS INT) AS d1p1w10
      ,CAST(isnull(dane.d2w1k1, 0) AS INT) AS d2w1k1
      ,CAST(isnull(dane.d2w1k2, 0) AS INT) AS d2w1k2
	  ,CAST(isnull(dane.d2w1k3, 0) AS INT) AS d2w1k3 --nowe pole na formularzu za rok 2020, zmienić argument funkcji 'isnull()' na właściwe pole
      ,CAST(isnull(dane.d2w2k1, 0) AS INT) AS d2w2k1
      ,CAST(isnull(dane.d2w2k2, 0) AS INT) AS d2w2k2
	  ,CAST(isnull(dane.d2w2k3, 0) AS INT) AS d2w2k3 --nowe pole na formularzu za rok 2020, zmienić argument funkcji 'isnull()' na właściwe pole
