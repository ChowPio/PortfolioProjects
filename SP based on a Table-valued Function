      ,dane.email_nowy
      --,dane.tl_o
      --,dane.osoba
      --,dane.e_mail_o
      --,dane.czas1
      --,dane.czas2
      ,dane.ids
      ,CASE WHEN isnull(dane.d2w1k1, 0) <> 0 AND (isnull(dane.d3w01k3, 0) <> 0 or isnull(dane.d3w28k3, 0) <> 0) THEN 1
            WHEN isnull(dane.d2w1k1, 0) <> 0 AND isnull(dane.d3w01k3, 0) = 0 AND isnull(dane.d3w28k3, 0) = 0 THEN 2
            WHEN isnull(dane.d2w1k1, 0) = 0 THEN 3
            END AS ParameterCaption




	  ,right([podzial_teryt].[makreg],1) as region
      ,[podzial_teryt].[woj]
      ,[podzial_teryt].[podr] as podregion
      ,[podzial_teryt].[pow]
      ,[podzial_teryt].[gmi]
      ,[podzial_teryt].[rodz]
      ,replace([podzial_teryt].[makreg_nazwa], left([podzial_teryt].[makreg_nazwa],12), '') as region_nazwa
      ,[podzial_teryt].[woj_nazwa]
      ,replace([podzial_teryt].[podr_nazwa], left([podzial_teryt].[podr_nazwa],12), '') as podregion_nazwa
      ,[podzial_teryt].[pow_nazwa]
      ,[podzial_teryt].[gmi_nazwa] as gmina_nazwa,
	  null as nazdod

	  ,dbo.slownik_kod_rpz.kod_rpz_nazwa
      ,kod_podmiotu.nazwa AS kod_podmiotu_nazwa

	  , case 
		when dane.woj + dane.pow + dane.gmi  in (select woj_pow_gmi from [spd].dbo.v_teryt_metropolia) then 1
		else 0
	  end as metropolia




FROM SPD.dbo.zd3_2021_dane AS dane                            -- !!! zamienić na poprawny widok, gdy dane będą już dostępne
LEFT OUTER JOIN dbo.slownik_kod_rpz
    ON dane.kod_rpz = dbo.slownik_kod_rpz.id
LEFT OUTER JOIN SPD.[dbo].[v_teryt_nuts_2021] AS [podzial_teryt] -- !!! zamienić na poprawny widok, gdy dane będą już dostępne. 
    ON dane.woj = podzial_teryt.woj AND                          -- Od roku 2020 wykorzystujemy do łączenia z danymi terytorialnymi tabelę NUTS (a nie NTS jak to było za rok 2019)
       dane.pow = podzial_teryt.pow AND
       dane.gmi = podzial_teryt.gmi AND
       dane.rodz = podzial_teryt.rodz
LEFT OUTER JOIN SPD.dbo.slownik_zd_kod_podmiotu AS kod_podmiotu
    ON dane.kod_podm = kod_podmiotu.symbol
--LEFT OUTER JOIN [dbo].[30_VI_2015] AS ludnosc_30_VI
--    ON dane.woj = ludnosc_30_VI.woj AND
--       dane.pow = ludnosc_30_VI.pow AND
--       dane.gmi = LEFT(ludnosc_30_VI.gmi, 2) AND
--       dane.rodz = RIGHT(ludnosc_30_VI.gmi, 1)
--LEFT OUTER JOIN [dbo].[31_XII_2015] AS ludnosc_31_XII
--    ON dane.woj = ludnosc_31_XII.woj AND
--       dane.pow = ludnosc_31_XII.pow AND
--       dane.gmi = LEFT(ludnosc_31_XII.gmi, 2) AND
--       dane.rodz = RIGHT(ludnosc_31_XII.gmi, 1)



--select *  FROM SPD.dbo.zd3_2020_dane

GO







--FINALLY, THE CODE OF A STORED PROCEDURE. Also the code of a function used in the definition of the SP.


USE [SZ_ZD3]
GO

/****** Object:  StoredProcedure [dbo].[spzd3_2020_wynikowa_44A4B]    Script Date: 2022-02-11 11:57:20 ******/
SET ANSI_NULLS ON
GO

SET QUOTED_IDENTIFIER ON
GO


CREATE PROC [dbo].[spzd3_2021_wynikowa_44A4B] (@Rodzaj_poradni AS VARCHAR(2) = '1', @Char_miejsc AS VARCHAR(MAX) = null)
WITH RECOMPILE
AS
Select

'2021' as rok_sprawozdania,

ra, rodz, fw, kod_zl, kod_rpz,

--makreg,
podregion,
podregion_nazwa,
woj, 
woj_nazwa, 
region,
pow,
pow_nazwa, 
gmi, 
gmina_nazwa,

            CASE 
		   WHEN @Rodzaj_poradni = '1' THEN d3w01k2 
		   WHEN @Rodzaj_poradni = '2' THEN d3w02k2
		   WHEN @Rodzaj_poradni = '3' THEN d3w03k2
		   WHEN @Rodzaj_poradni = '4' THEN d3w04k2 
		   WHEN @Rodzaj_poradni = '5' THEN d3w05k2 
             WHEN @Rodzaj_poradni = '6' THEN d3w06k2 
		   WHEN @Rodzaj_poradni = '7' THEN d3w07k2 
		   WHEN @Rodzaj_poradni = '8' THEN d3w08k2
		   WHEN @Rodzaj_poradni = '9' THEN d3w09k2 
		   WHEN @Rodzaj_poradni = '10' THEN d3w10k2
		   WHEN @Rodzaj_poradni = '11' THEN d3w11k2 
		   WHEN @Rodzaj_poradni = '12' THEN d3w12k2
		   WHEN @Rodzaj_poradni = '13' THEN d3w13k2
		   WHEN @Rodzaj_poradni = '14' THEN d3w14k2 
		   WHEN @Rodzaj_poradni = '15' THEN d3w15k2 
		   WHEN @Rodzaj_poradni = '16' THEN d3w16k2
		   WHEN @Rodzaj_poradni = '17' THEN d3w17k2
		   WHEN @Rodzaj_poradni = '18' THEN d3w18k2 
		   WHEN @Rodzaj_poradni = '19' THEN d3w19k2
		   WHEN @Rodzaj_poradni = '20' THEN d3w20k2
		   WHEN @Rodzaj_poradni = '21' THEN d3w21k2 
		   WHEN @Rodzaj_poradni = '22' THEN d3w22k2 
		   WHEN @Rodzaj_poradni = '23' THEN d3w23k2
		   WHEN @Rodzaj_poradni = '24' THEN d3w24k2
		   WHEN @Rodzaj_poradni = '25' THEN d3w25k2
		   WHEN @Rodzaj_poradni = '26' THEN d3w26k2 
		   WHEN @Rodzaj_poradni = '27' THEN d3w27k2 END AS kol1,

	 	-- kolumna 2 raportu
            CASE 
		   WHEN @Rodzaj_poradni = '1' THEN d3w01k3 
		   WHEN @Rodzaj_poradni = '2' THEN d3w02k3
		   WHEN @Rodzaj_poradni = '3' THEN d3w03k3
		   WHEN @Rodzaj_poradni = '4' THEN d3w04k3 
		   WHEN @Rodzaj_poradni = '5' THEN d3w05k3 
             WHEN @Rodzaj_poradni = '6' THEN d3w06k3 
		   WHEN @Rodzaj_poradni = '7' THEN d3w07k3 
		   WHEN @Rodzaj_poradni = '8' THEN d3w08k3
		   WHEN @Rodzaj_poradni = '9' THEN d3w09k3 
		   WHEN @Rodzaj_poradni = '10' THEN d3w10k3
		   WHEN @Rodzaj_poradni = '11' THEN d3w11k3 
		   WHEN @Rodzaj_poradni = '12' THEN d3w12k3
		   WHEN @Rodzaj_poradni = '13' THEN d3w13k3
		   WHEN @Rodzaj_poradni = '14' THEN d3w14k3 
		   WHEN @Rodzaj_poradni = '15' THEN d3w15k3 
		   WHEN @Rodzaj_poradni = '16' THEN d3w16k3
		   WHEN @Rodzaj_poradni = '17' THEN d3w17k3
		   WHEN @Rodzaj_poradni = '18' THEN d3w18k3 
		   WHEN @Rodzaj_poradni = '19' THEN d3w19k3
		   WHEN @Rodzaj_poradni = '20' THEN d3w20k3
		   WHEN @Rodzaj_poradni = '21' THEN d3w21k3 
		   WHEN @Rodzaj_poradni = '22' THEN d3w22k3 
		   WHEN @Rodzaj_poradni = '23' THEN d3w23k3
		   WHEN @Rodzaj_poradni = '24' THEN d3w24k3
		   WHEN @Rodzaj_poradni = '25' THEN d3w25k3
		   WHEN @Rodzaj_poradni = '26' THEN d3w26k3 
		   WHEN @Rodzaj_poradni = '27' THEN d3w27k3 END AS kol2,

		  -- kolumna 3 raportu
            CASE 
		   WHEN @Rodzaj_poradni = '1' THEN d3w01k4
		   WHEN @Rodzaj_poradni = '2' THEN d3w02k4
		   WHEN @Rodzaj_poradni = '3' THEN d3w03k4
		   WHEN @Rodzaj_poradni = '4' THEN d3w04k4 
		   WHEN @Rodzaj_poradni = '5' THEN d3w05k4 
             WHEN @Rodzaj_poradni = '6' THEN 0 
		   WHEN @Rodzaj_poradni = '7' THEN d3w07k4 
		   WHEN @Rodzaj_poradni = '8' THEN d3w08k4
		   WHEN @Rodzaj_poradni = '9' THEN d3w09k4 
		   WHEN @Rodzaj_poradni = '10' THEN d3w10k4
		   WHEN @Rodzaj_poradni = '11' THEN d3w11k4 
		   WHEN @Rodzaj_poradni = '12' THEN d3w12k4
		   WHEN @Rodzaj_poradni = '13' THEN d3w13k4
		   WHEN @Rodzaj_poradni = '14' THEN d3w14k4 
		   WHEN @Rodzaj_poradni = '15' THEN d3w15k4 
		   WHEN @Rodzaj_poradni = '16' THEN d3w16k4
		   WHEN @Rodzaj_poradni = '17' THEN d3w17k4
		   WHEN @Rodzaj_poradni = '18' THEN d3w18k4 
		   WHEN @Rodzaj_poradni = '19' THEN d3w19k4
		   WHEN @Rodzaj_poradni = '20' THEN d3w20k4
		   WHEN @Rodzaj_poradni = '21' THEN d3w21k4 
		   WHEN @Rodzaj_poradni = '22' THEN d3w22k4 
		   WHEN @Rodzaj_poradni = '23' THEN d3w23k4
		   WHEN @Rodzaj_poradni = '24' THEN d3w24k4
		   WHEN @Rodzaj_poradni = '25' THEN d3w25k4
		   WHEN @Rodzaj_poradni = '26' THEN d3w26k4 
		   WHEN @Rodzaj_poradni = '27' THEN d3w27k4 END AS kol3,

		   -- kolumna 4 raportu
            CASE 
		   WHEN @Rodzaj_poradni = '1' THEN d3w01k5
		   WHEN @Rodzaj_poradni = '2' THEN d3w02k5
		   WHEN @Rodzaj_poradni = '3' THEN d3w03k5
		   WHEN @Rodzaj_poradni = '4' THEN d3w04k5
		   WHEN @Rodzaj_poradni = '5' THEN d3w05k5 
             WHEN @Rodzaj_poradni = '6' THEN d3w06k5
		   WHEN @Rodzaj_poradni = '7' THEN d3w07k5 
		   WHEN @Rodzaj_poradni = '8' THEN d3w08k5
		   WHEN @Rodzaj_poradni = '9' THEN d3w09k5 
		   WHEN @Rodzaj_poradni = '10' THEN d3w10k5
		   WHEN @Rodzaj_poradni = '11' THEN d3w11k5 
		   WHEN @Rodzaj_poradni = '12' THEN d3w12k5
		   WHEN @Rodzaj_poradni = '13' THEN d3w13k5
		   WHEN @Rodzaj_poradni = '14' THEN d3w14k5
		   WHEN @Rodzaj_poradni = '15' THEN d3w15k5 
		   --po podstawieniu view za rok 2017 zamienić poniżej na pole 'd3w16k5' (wiersz 16 - medycyna sportowa osób 65+)  
		   WHEN @Rodzaj_poradni = '16' THEN d3w16k5
		   WHEN @Rodzaj_poradni = '17' THEN 0
		   WHEN @Rodzaj_poradni = '18' THEN d3w18k5 
		   WHEN @Rodzaj_poradni = '19' THEN d3w19k5
		   WHEN @Rodzaj_poradni = '20' THEN d3w20k5
		   WHEN @Rodzaj_poradni = '21' THEN d3w21k5 
		   WHEN @Rodzaj_poradni = '22' THEN d3w22k5 
		   WHEN @Rodzaj_poradni = '23' THEN d3w23k5
		   WHEN @Rodzaj_poradni = '24' THEN d3w24k5
		   WHEN @Rodzaj_poradni = '25' THEN d3w25k5
		   WHEN @Rodzaj_poradni = '26' THEN d3w26k5 
		   WHEN @Rodzaj_poradni = '27' THEN d3w27k5 END AS kol4,

		   -- kolumna 5 raportu
            CASE 
		   WHEN @Rodzaj_poradni = '1' THEN d3w01k6
		   WHEN @Rodzaj_poradni = '2' THEN d3w02k6
		   WHEN @Rodzaj_poradni = '3' THEN d3w03k6
		   WHEN @Rodzaj_poradni = '4' THEN d3w04k6
		   WHEN @Rodzaj_poradni = '5' THEN d3w05k6 
             WHEN @Rodzaj_poradni = '6' THEN d3w06k6
		   WHEN @Rodzaj_poradni = '7' THEN d3w07k6 
		   WHEN @Rodzaj_poradni = '8' THEN d3w08k6
		   WHEN @Rodzaj_poradni = '9' THEN d3w09k6 
		   WHEN @Rodzaj_poradni = '10' THEN d3w10k6
		   WHEN @Rodzaj_poradni = '11' THEN d3w11k6 
		   WHEN @Rodzaj_poradni = '12' THEN d3w12k6
		   WHEN @Rodzaj_poradni = '13' THEN d3w13k6
		   WHEN @Rodzaj_poradni = '14' THEN d3w14k6
		   WHEN @Rodzaj_poradni = '15' THEN d3w15k6 
		   WHEN @Rodzaj_poradni = '16' THEN d3w16k6
		   WHEN @Rodzaj_poradni = '17' THEN d3w17k6
		   WHEN @Rodzaj_poradni = '18' THEN d3w18k6 
		   WHEN @Rodzaj_poradni = '19' THEN d3w19k6
		   WHEN @Rodzaj_poradni = '20' THEN d3w20k6
		   WHEN @Rodzaj_poradni = '21' THEN d3w21k6		    
		   WHEN @Rodzaj_poradni = '22' THEN d3w22k6		    
		   WHEN @Rodzaj_poradni = '23' THEN d3w23k6
		   WHEN @Rodzaj_poradni = '24' THEN d3w24k6
		   WHEN @Rodzaj_poradni = '25' THEN d3w25k6
		   WHEN @Rodzaj_poradni = '26' THEN d3w26k6 
		   WHEN @Rodzaj_poradni = '27' THEN d3w27k6 END AS kol5,
           
		  -- kolumna 6 raportu
            CASE 
		   WHEN @Rodzaj_poradni = '1' THEN d3w01k7
		   WHEN @Rodzaj_poradni = '2' THEN d3w02k7
		   WHEN @Rodzaj_poradni = '3' THEN d3w03k7
		   WHEN @Rodzaj_poradni = '4' THEN d3w04k7
		   WHEN @Rodzaj_poradni = '5' THEN d3w05k7 
             WHEN @Rodzaj_poradni = '6' THEN d3w06k7
		   WHEN @Rodzaj_poradni = '7' THEN d3w07k7 
		   WHEN @Rodzaj_poradni = '8' THEN d3w08k7
		   WHEN @Rodzaj_poradni = '9' THEN d3w09k7 
		   WHEN @Rodzaj_poradni = '10' THEN d3w10k7
		   WHEN @Rodzaj_poradni = '11' THEN d3w11k7 
		   WHEN @Rodzaj_poradni = '12' THEN d3w12k7
		   WHEN @Rodzaj_poradni = '13' THEN d3w13k7
		   WHEN @Rodzaj_poradni = '14' THEN d3w14k7
		   WHEN @Rodzaj_poradni = '15' THEN d3w15k7 
		   WHEN @Rodzaj_poradni = '16' THEN d3w16k7
		   WHEN @Rodzaj_poradni = '17' THEN d3w17k7
		   WHEN @Rodzaj_poradni = '18' THEN d3w18k7 
		   WHEN @Rodzaj_poradni = '19' THEN d3w19k7
		   WHEN @Rodzaj_poradni = '20' THEN d3w20k7
		   WHEN @Rodzaj_poradni = '21' THEN d3w21k7		    
		   WHEN @Rodzaj_poradni = '22' THEN d3w22k7		    
		   WHEN @Rodzaj_poradni = '23' THEN d3w23k7
		   WHEN @Rodzaj_poradni = '24' THEN d3w24k7
		   WHEN @Rodzaj_poradni = '25' THEN d3w25k7
		   WHEN @Rodzaj_poradni = '26' THEN d3w26k7 
		   WHEN @Rodzaj_poradni = '27' THEN d3w27k7 END AS kol6,

		 

		 --PRZYCHODNIE   
	 	
		--kolumna 1 raportu
            CASE 
		   WHEN @Rodzaj_poradni = '1' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w01k2 
		   WHEN @Rodzaj_poradni = '2' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w02k2
		   WHEN @Rodzaj_poradni = '3' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w03k2
		   WHEN @Rodzaj_poradni = '4' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w04k2 
		   WHEN @Rodzaj_poradni = '5' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w05k2 
             WHEN @Rodzaj_poradni = '6' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w06k2 
		   WHEN @Rodzaj_poradni = '7' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w07k2 
		   WHEN @Rodzaj_poradni = '8' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w08k2
		   WHEN @Rodzaj_poradni = '9' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w09k2 
		   WHEN @Rodzaj_poradni = '10' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w10k2
		   WHEN @Rodzaj_poradni = '11' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w11k2 
		   WHEN @Rodzaj_poradni = '12' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w12k2
		   WHEN @Rodzaj_poradni = '13' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w13k2
		   WHEN @Rodzaj_poradni = '14' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w14k2 
		   WHEN @Rodzaj_poradni = '15' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w15k2 
		   WHEN @Rodzaj_poradni = '16' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w16k2
		   WHEN @Rodzaj_poradni = '17' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w17k2
		   WHEN @Rodzaj_poradni = '18' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w18k2 
		   WHEN @Rodzaj_poradni = '19' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w19k2
		   WHEN @Rodzaj_poradni = '20' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w20k2
		   WHEN @Rodzaj_poradni = '21' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w21k2 
		   WHEN @Rodzaj_poradni = '22' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w22k2 
		   WHEN @Rodzaj_poradni = '23' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w23k2
		   WHEN @Rodzaj_poradni = '24' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w24k2
		   WHEN @Rodzaj_poradni = '25' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w25k2
		   WHEN @Rodzaj_poradni = '26' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w26k2 
		   WHEN @Rodzaj_poradni = '27' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w27k2 END AS kol1_przych,
		
		-- kolumna 2 raportu
            CASE 
		   WHEN @Rodzaj_poradni = '1' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w01k3 
		   WHEN @Rodzaj_poradni = '2' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w02k3
		   WHEN @Rodzaj_poradni = '3' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w03k3
		   WHEN @Rodzaj_poradni = '4' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w04k3 
		   WHEN @Rodzaj_poradni = '5' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w05k3 
             WHEN @Rodzaj_poradni = '6' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w06k3 
		   WHEN @Rodzaj_poradni = '7' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w07k3 
		   WHEN @Rodzaj_poradni = '8' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w08k3
		   WHEN @Rodzaj_poradni = '9' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w09k3 
		   WHEN @Rodzaj_poradni = '10' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w10k3
		   WHEN @Rodzaj_poradni = '11' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w11k3 
		   WHEN @Rodzaj_poradni = '12' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w12k3
		   WHEN @Rodzaj_poradni = '13' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w13k3
		   WHEN @Rodzaj_poradni = '14' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w14k3 
		   WHEN @Rodzaj_poradni = '15' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w15k3 
		   WHEN @Rodzaj_poradni = '16' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w16k3
		   WHEN @Rodzaj_poradni = '17' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w17k3
		   WHEN @Rodzaj_poradni = '18' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w18k3 
		   WHEN @Rodzaj_poradni = '19' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w19k3
		   WHEN @Rodzaj_poradni = '20' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w20k3
		   WHEN @Rodzaj_poradni = '21' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w21k3 
		   WHEN @Rodzaj_poradni = '22' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w22k3 
		   WHEN @Rodzaj_poradni = '23' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w23k3
		   WHEN @Rodzaj_poradni = '24' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w24k3
		   WHEN @Rodzaj_poradni = '25' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w25k3
		   WHEN @Rodzaj_poradni = '26' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w26k3 
		   WHEN @Rodzaj_poradni = '27' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w27k3 END AS kol2_przych,

		  -- kolumna 3 raportu
            CASE 
		   WHEN @Rodzaj_poradni = '1' AND KOD_RPZ IS NULL AND KOD_ZL IS NULL THEN  d3w01k4
